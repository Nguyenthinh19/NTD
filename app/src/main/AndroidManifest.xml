<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ntd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".lovecandidate.LoveCandidateActivity"></activity>
        <activity android:name=".candidateinviteinterview.CandidateInviteInterviewActivity" />
        <activity android:name=".candidatesavejob.CandidateSaveJobActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".login.activities.LoginEditAcitvity" />
        <activity android:name=".login.activities.LoginSucessActivity" />
        <activity android:name=".login.activities.LoginIDActivity" />
        <activity
            android:name=".login.activities.LoginActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>